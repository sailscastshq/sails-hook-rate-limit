{
  "name": "sails-hook-rate-limit",
  "version": "0.0.1",
  "description": "Sails hook for rate limiting",
  "main": "index.js",
  "sails": {
    "isHook": true,
    "hookName": "rate-limit"
  },
  "scripts": {
    "lint": "npx prettier --check .",
    "lint:fix": "npx prettier --write .",
    "prepare": "husky install"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/DominusKelvin/treblle-sails.git"
  },
  "keywords": [
    "sails",
    "rate-limit",
    "rate limiting"
  ],
  "author": "Kelvin Omereshone<kelvinomereshone@gmail.com>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/sailscastshq/sails-hook-rate-limit/issues"
  },
  "homepage": "https://github.com/sailscastshq/sails-hook-rate-limit#readme",
  "peerDependencies": {
    "sails": ">=1"
  },
  "dependencies": {
    "express-rate-limit": "^6.7.0"
  },
  "devDependencies": {
    "husky": "^8.0.3",
    "lint-staged": "^13.1.0",
    "prettier": "2.8.3"
  },
  "lint-staged": {
    "**/*": "prettier --write --ignore-unknown"
  }
